<% if user_signed_in? %>
    <%= button_to(
      "Log Out",
      destroy_user_session_path,
      method: :delete,
      class: "btn btn-primary"
    )%>
  <h1> User list </h1>
    <% @users.each do |user|%>
        <div>
            <%= link_to user.name, user_path(user) %>
        </div>
    <% end %>
    <% else %>
      <a href="users/sign_in">Sign In</a>
<% end %>